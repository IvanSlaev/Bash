
#!/bin/bash

dispTime=0
procTime=0

a="a"
b="a"

while read line; do
	IFS=' ' read -a array <<<$line
	for index in "${!array[@]}"; do
		if [ "${array[index]}" = "Display:" ]; then
			dispTime=$(($dispTime+${array[index+1]}))
		fi
		if [ "${array[index]}" = "Proc:" ]; then
			procTime=$(($procTime+${array[index+1]}))
		fi
	done
done < test.txt

echo "Display works: $dispTime"
echo "Proccessor works: $procTime"
